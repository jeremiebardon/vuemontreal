{
  "builds": [
    {
      "src": "package.json", "use": "@now/static-build"
    }
  ],
  "env": {
    "STORYBLOK_TOKEN": "@secret-sb",
    "GOOGLE_ANALYTIC": "@secret-ga",
    "STORYBLOK_VERSION": "@secret-sv"
  },
  "build": {
    "env": {
      "STORYBLOK_TOKEN": "@secret-sb",
      "GOOGLE_ANALYTIC": "@secret-ga",
      "STORYBLOK_VERSION": "@secret-sv"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "200.html" }
  ]
}
